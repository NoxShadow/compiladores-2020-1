<Page
    x:Class="compiladores20201.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:compiladores20201"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    MinWidth="900"
    MinHeight="600"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

  <Grid>

    <Grid.RowDefinitions>
      <RowDefinition Height="2.333*" />
      <RowDefinition Height="13.333*" />
      <RowDefinition Height="3.333*" />
      <RowDefinition Height="1*" />
    </Grid.RowDefinitions>

    <TextBlock
      Grid.Row="0"
      Text="Primeira parte"
      />

    <TextBlock
      Grid.Row="1"
      Text="Segunda parte"
      />

    <TextBlock
      Grid.Row="2"
      Text="Terceira parte"
      />

    <TextBlock
      Grid.Row="3"
      Text="Quarta parte"
      />

  </Grid>
  
</Page>
